extends layout

block content
  h4= title

  ul#dashboard

  script 
    var socket = io.connect('http://localhost:3000');
    socket.on('connect', function () {
      socket.on('hit', function (msg) {
        $('#dashboard').append('<li>' + msg.time + ': ' + msg.browser + ' ' + msg.page + '</li>');
      });
    });
